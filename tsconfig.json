{
    "compilerOptions": {
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "allowJs": true,
      "strict": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "baseUrl": "./src",
      "paths": {
        "@/*": ["*"]
      }
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
  